@model List<Trainee>
@{
    ViewData["Title"] = "View";
}

<h1 class="text-center ">Trainee data</h1>

<table class="table table-hover table-bordered">
    <tr >
        <th> Trainee Id</th>
        <th>Trainee Name</th>
        <th>grade</th>
        <th>Action</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            
                <td>@item.Grade</td>
        
            <td><a class="btn btn-primary" href="/Trainee/details/@item.Id">View</a></td>
        </tr>

    }

</table>


